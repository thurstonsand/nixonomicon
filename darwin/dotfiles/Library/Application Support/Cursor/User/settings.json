{
    "editor.fontFamily": "Mononoki Nerd Font, monospace",
    "editor.fontSize": 13,
    "editor.wordWrap": "on",
    "workbench.editor.wrapTabs": true,
    "editor.minimap.enabled": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "workbench.commandPalette.experimental.suggestCommands": true,
    "workbench.experimental.cloudChanges.autoStore": "onShutdown",
    "workbench.experimental.cloudChanges.partialMatches.enabled": true,
    "workbench.experimental.share.enabled": true,
    "workbench.list.smoothScrolling": true,
    "git.allowForcePush": true,
    "git.autofetch": "all",
    "git.autoStash": true,
    "git.branchProtection": [
        "main"
    ],
    "git.branchProtectionPrompt": "alwaysCommit",
    "git.confirmForcePush": false,
    "git.defaultCloneDirectory": "/Users/thurstonsand/Develop",
    "git.terminalAuthentication": false,
    "git.terminalGitEditor": true,
    "github.gitProtocol": "ssh",
    "merge-conflict.autoNavigateNextConflict.enabled": true,
    "editor.formatOnSave": true,
    "nix.formatterPath": [
        "nix",
        "fmt",
    ],
    "nix.enableLanguageServer": true,
    "nix.serverPath": "nil",
    "nix.serverSettings": {
        "nil": {
            "formatting": {
                "command": [
                    "nix",
                    "fmt",
                    "--",
                    "--"
                ]
            }
        },
    },
    "git.confirmSync": false,
    "explorer.confirmDelete": false,
    "workbench.colorTheme": "Monokai",
    "files.autoSave": "onFocusChange",
    "window.commandCenter": true,
    "cursor.cpp.enablePartialAccepts": true,
    "cursor.cmdk.useThemedDiffBackground": true,
    "cursor.composer.cmdPFilePicker2": true,
    "cursor.diffs.useCharacterLevelDiffs": true,
    "cursor.general.enableShadowWorkspace": true,
    "cursor.general.gitGraphIndexing": "enabled",
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "diffEditor.codeLens": true,
    "cursor.aipreview.enabled": true,
    "cursor.composer.renderPillsInsteadOfBlocks": true
}